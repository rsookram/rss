CREATE TABLE Feed (
    id INTEGER PRIMARY KEY,
    url TEXT NOT NULL UNIQUE,
    name TEXT NOT NULL
);

CREATE INDEX feedUrl ON Feed(url);

getLastCreatedId:
SELECT last_insert_rowid();

feed:
SELECT id, url, name FROM Feed ORDER BY url;

insert:
INSERT INTO Feed(url, name) VALUES (?, ?);

delete:
DELETE FROM Feed
WHERE id = ?;
